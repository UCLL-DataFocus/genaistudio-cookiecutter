{
  "app_name": "What is the name of the app? (genaistudio- will be added by default)",
  "openproject_url": "https://ucll.openproject.com/projects/genai-studio/",
  "app_description": "A Python app that reticulates splines.",
  "app_lead_name": "John Smith",
  "researchers_names": "Bob Johnson, Alice Brown, Charlie White",
  "python_version": "3.10.9",
  "with_streamlit": "1",
  "development_environment": [
    "simple",
    "strict"
  ],
  "with_conventional_commits": "{% if cookiecutter.development_environment == 'simple' %}0{% else %}1{% endif %}",
  "with_fastapi_api": "0",
  "with_typer_cli": "0",
  "continuous_integration": [
    "GitHub",
    "GitLab"
  ],
  "include_speech": "0",
  "__docker_image": "python:$PYTHON_VERSION-slim",
  "__docstring_style": "NumPy",
  "__project_name": "genaistudio-{{ cookiecutter.app_name }}",
  "__project_name_kebab_case": "genaistudio-{{ cookiecutter.app_name|slugify(separator='-') }}",
  "__project_name_snake_case": "genaistudio-{{ cookiecutter.app_name|slugify(separator='_') }}"
}